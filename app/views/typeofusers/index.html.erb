<p id="notice"><%= notice %></p>

<h1>Tipo de usuario</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @typeofusers.each do |typeofuser| %>
      <tr>
        <td><%= typeofuser.name %></td>
        <td><%= link_to 'Ver', typeofuser %></td>
        <td><%= link_to 'Editar', edit_typeofuser_path(typeofuser) %></td>
        <td><%= link_to 'Eliminar', typeofuser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Tipo de usuario', new_typeofuser_path %>
