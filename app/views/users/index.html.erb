<h1>Usuarios Registrados</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Username</th>
      <th>Correo</th>
      <th>Tipo de Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.typeofuser.name %></td>
        <td><%= link_to 'Ver',user %></td>
        <td><%= link_to 'Editar', edit_user_path(user)%></td>
        <td><%= link_to 'Eliminar', user,method: :delete, data: { confirm: 'Estas Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

